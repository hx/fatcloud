package  {		import flash.events.*;		public class DOMTagEvent extends Event {		public static const FOCUS:String='focus';		public static const BLUR:String='blur';		public static const SELECT:String='select';				public var tag:Object;		public var tagIndex:uint;		public function DOMTagEvent(tag:Object, tagIndex:uint, eventName:String) {			super(eventName);			this.tag=tag;			this.tagIndex=tagIndex;		}		}	}